@page
@model ShoppingList.Pages.LoginPage2

@{
    Layout = Layout;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <title></title>
</head>
<body>
<div>
        <form method="post">
            <div>
                <label for="username">Username:</label>
                <input type="text" id="username" name="username">
            </div>
            <div>
                <label for="password">Password:</label>
                <input type="password" id="password" name="password">
            </div>
            <div>
                <label for="shoppingListId">Shopping list id:</label>
                <input type="number" id="shoppingListId" name="shoppingListId">
            </div>
            <div>
                <button id="submit">Sign in</button>
            </div>
        </form>
</div>
</body>
</html>